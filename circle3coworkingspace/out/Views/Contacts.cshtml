@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = "masterTemplate.cshtml";
}



<div class="site-content">
    <div class="content-wrapper">

        <!--=================================
        Contact Us -->
        <section class="contact-map">
            <iframe class="map" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3453.1871786859974!2d31.348637723824947!3d30.06016871782439!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14583ffe5986878f%3A0x37a5f6ee9c0288ca!2sCircle%203%20Coworking%20space!5e0!3m2!1sar!2seg!4v1752287283448!5m2!1sar!2seg" style="border:0; width: 100%;" allowfullscreen=""></iframe>
        </section>
        <section class="space-ptb section-bg-effect">
            <div class="container">
                <div class="row">
                    <div class="col-lg-8">
                        <div class="section-title pe-0 me-0 pe-md-5 me-lg-5">
                            <h2 class="title">@Model.Value("formTitle")</h2>
                            <p>@Model.Value("formDesc")</p>
                        </div>
                        @await Component.InvokeAsync("Contact")

                    </div>
                    <div class="col-lg-1 d-none d-xl-block"></div>
                    <div class="col-xl-3 col-lg-4 ps-3 ps-xl-0 ps-lg-5 mt-5 mt-lg-0 pt-4 pt-lg-0">
                        <h4 class="title">Our offices</h4>
                        <div class="address-info-wrapper mt-3 pt-3">
                            <h4 class="address-info-title">France</h4>
                            <ul class="address-info-list">
                                <li><i class="icon"><img class="img-fluid" src="images/icon-location.png" alt="" /></i>@Model.Value("contactsAddress")</li>
                                <li><i class="icon"><img class="img-fluid" src="images/icon-phone-call.png" alt="" /></i>@Model.Value("contactsNumber")</li>
                                <li><i class="icon"><img class="img-fluid" src="images/icon-email.png" alt="" /></i>@Model.Value("contactsEmail")</li>
                            </ul>
                        </div>
                        <div class="address-info-wrapper mt-5">
                            <h4 class="address-info-title">Opening Hours</h4>
                            <ul class="address-info-list">
                                <li>@Model.Value("contactsOpeningHours")</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </section>

    </div>
</div>