                    <div class="col-xl-5 mb-lg-5 text-center text-xl-start mb-5 mb-xl-0">
                        <img class="img-fluid" src="@AboutDetailsImageHome?.MediaUrl()" alt="25 Years Expertise" />
                    </div>
                    <div class="col-xl-7 ps-xl-5 pe-lg-5">
                        <div class="section-title pe-xxl-5 me-xxl-5"><h2 class="title">@AboutPage.Value("aboutDetailsTitle")</h2></div>
                        <div class="ps-xxl-5 ms-0 ms-md-5">
                            <div class="row">
                                <div class="col-lg-10 mb-3">
                                    <p class="text-white">@AboutPage.Value("aboutDetailsDesc")</p>
                                </div>
                            </div>
                            <div class="row align-items-center">
                                <div class="col-md-9">
                                    <div class="list-wrapper list-light">
                                        @{
                                            if (AboutPage.Value<string[]>("aboutDetailsPoints").Length > 0)
                                            {
                                                <ul class="list col-2">
                                                    @foreach (var item in AboutPage.Value<string[]>("aboutDetailsPoints"))
                                                    {
                                                        <li><i class="fa-regular fa-snowflake"></i> @item</li>
                                                    }
                                                </ul>
                                            }
                                        }

                                        
                                    </div>
                                </div>
                                <div class="col-md-3 mt-5 mt-md-0">
                                    <a class="btn btn-effect" href="@AboutPage.Url()"><span>About Us</span></a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row mt-5">
                    <div class="col-lg-3 col-sm-6 mt-4 text-center">
                        <div class="counter counter-style-2 counter-light">
                            <div class="counter-number"><span class="timer mb-0" data-to="5" data-speed="2000">@AboutPage.Value("aboutDetailsNo1")</span><span class="suffix">M</span></div>
                            <div class="counter-info"><span class="counter-title text-white">@AboutPage.Value("aboutDetailsNo11")</span></div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6 mt-4 text-center">
                        <div class="counter counter-style-2 counter-light">
                            <div class="counter-number"><span class="timer mb-0" data-to="30" data-speed="2000">@AboutPage.Value("aboutDetailsNo2")</span><span class="suffix">K+</span></div>
                            <div class="counter-info"><span class="counter-title text-white">@AboutPage.Value("aboutDetailsNo2")</span></div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6 mt-4 text-center">
                        <div class="counter counter-style-2 counter-light">
                            <div class="counter-number"><span class="timer mb-0" data-to="10" data-speed="2000">@AboutPage.Value("aboutDetailsNo3")</span><span class="suffix">K+</span></div>
                            <div class="counter-info"><span class="counter-title text-white">@AboutPage.Value("aboutDetailsNo33")</span></div>
                        </div>
                    </div>
                    <div class="col-lg-3 col-sm-6 mt-4 text-center">
                        <div class="counter counter-style-2 counter-light">
                            <div class="counter-number"><span class="timer mb-0" data-to="110" data-speed="2000">@AboutPage.Value("aboutDetailsNo4")</span><span class="suffix">+</span></div>
                            <div class="counter-info"><span class="counter-title text-white">@AboutPage.Value("aboutDetailsNo44")</span></div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

