        Blog -->
        <section class="space-ptb section-bg-effect">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-xxl-8 col-xl-10 text-center">
                        <div class="section-title">
                            <h2 class="title">@Model.Value("homeBlogTitle")</h2>
                            <p>@Model.Value("homeBlogDesc")</p>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="owl-carousel" data-loop="false" data-items="3" data-lg-items="3" data-md-items="2" data-sm-items="2" data-xs-items="1" data-space="30">
                           
                            
                            
                            @{
                                var selectionBlog = Umbraco.Content(Guid.Parse("7aca88ee-81ae-4ef6-b5ad-97a5f4afccdf"))
                                .ChildrenOfType("article")
                                .Where(x => x.IsVisible())
                                .OrderBy(x => x.CreateDate)
                                .Take(3);
                            }
                         
                                @foreach (var item in selectionBlog)
                                {
                                    var BlogImage = item.Value<MediaWithCrops>("heroImage");
                                    var blogDate = item.Value<DateTime>("date");
                                    

                                    <div class="item">
                                        <div class="blog-post-wrapper blog-style-1">
                                            <div class="blog-post-img"><img class="img-fluid" src="@BlogImage?.MediaUrl()" alt="" /></div>
                                            <div class="blog-post-info">
                                                <div class="post-meta">
                                                    <div class="post-meta-category"><a href="@item.Url()">@item.Value("articleCategory")</a></div>
                                                    <div class="post-meta-date"><a href="#">@blogDate.ToString("dd-MMM-yyyy")</a></div>
                                                </div>
                                                <h5 class="post-title"><a href="@item.Url()">@item.Value("title")</a></h5>
                                                <div class="post-link">
                                                    <a class="btn-arrow" href="#"><svg width="17" height="16" viewBox="0 0 17 16" fill="none" xmlns="http://www.w3.org/2000/svg"><g clip-path="url(#clip0_923_133)"><path d="M8.70801 0.959961L9.29825 2.7665C10.2512 5.68321 12.8308 7.77453 15.8928 8.1128C12.8468 8.37564 10.2578 10.4348 9.3276 13.3343L8.70801 15.2657" stroke="inherit" stroke-width="2" /><path d="M15.7602 8.12158H0.1875" stroke="inherit" stroke-width="2" /></g><defs><clipPath id="clip0_923_1331"><rect width="15.904" height="14.8437" fill="inherit" transform="translate(0.1875 0.578125)" /></clipPath></defs></svg></a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                            
                                }
                          


                       



                        </div>
                    </div>
                </div>
            </div>
        </section>

    </div>
